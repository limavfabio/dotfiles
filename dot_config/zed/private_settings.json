{
  "edit_predictions": {
    "provider": "zed",
  },
  "language_models": {
    "openai_compatible": {
      "ZAI": {
        "api_url": "https://api.z.ai/api/coding/paas/v4",
        "available_models": [
          {
            "name": "GLM-4.7",
            "max_tokens": 200000,
            "max_output_tokens": 32000,
            "max_completion_tokens": 200000,
            "capabilities": {
              "tools": true,
              "images": false,
              "parallel_tool_calls": false,
              "prompt_cache_key": false,
              "chat_completions": true,
            },
          },
        ],
      },
    },
  },
  "context_servers": {
    "tidewave-mcp": {
      "settings": {},

      "enabled": true,
      "url": "http://localhost:3000/tidewave/mcp",
    },
  },
  "agent": {
    "inline_assistant_model": {
      "provider": "ZAI",
      "model": "GLM-4.7",
    },
    "default_profile": "ask",
    "default_model": {
      "provider": "ZAI",
      "model": "GLM-4.7",
    },
    "use_modifier_to_send": false,
    "play_sound_when_agent_done": true,
    "always_allow_tool_actions": true,
  },
  "restore_on_startup": "last_session",
  "icon_theme": "Colored Zed Icons Theme Dark",
  "cursor_blink": false,
  "base_keymap": "VSCode",
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "buffer_font_family": "JetBrains Mono",
  "buffer_font_weight": 300,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Ayu Dark",
  },
  "languages": {
    "Ruby": {
      "language_servers": ["ruby-lsp", "rubocop", "!solargraph"],
    },
    "JavaScript": {
      "formatter": {
        "external": {
          "command": "oxfmt",
          "arguments": ["--stdin-filepath", "{buffer_path}"],
        },
      },
      "format_on_save": "on",
    },
    "TypeScript": {
      "formatter": {
        "external": {
          "command": "oxfmt",
          "arguments": ["--stdin-filepath", "{buffer_path}"],
        },
      },
      "format_on_save": "on",
    },
    "TSX": {
      "formatter": {
        "external": {
          "command": "oxfmt",
          "arguments": ["--stdin-filepath", "{buffer_path}"],
        },
      },
      "format_on_save": "on",
    },
  },
  "lsp": {
    "ruby-lsp": {
      "settings": {
        "use_bundler": true,
      },
      "initialization_options": {
        "enabledFeatures": {
          "diagnostics": false,
        },
      },
    },
    "rubocop": {
      "initialization_options": {
        "safeAutocorrect": false,
      },
    },
    "tailwindcss-language-server": {
      "settings": {
        "includeLanguages": {
          "erb": "html",
          "ruby": "html",
        },
        "experimental": {
          "classRegex": ["\\bclass:\\s*['\"]([^'\"]*)['\"]"],
        },
      },
    },
    "vscode-css-language-server": {
      "settings": {
        "css": {
          "lint": {
            "unknownAtRules": "ignore",
          },
        },
      },
    },
  },
  "auto_install_extensions": {
    "html": true,
    "dockerfile": true,
    "docker-compose": true,
    "ruby": true,
    "emmet": true,
    "ruff": true,
    "golangci-lint": true,
    "colored-zed-icons-theme": true,
    "templ": true,
    "mermaid": true,
    "toml": true,
    "git-firefly": true,
    "make": true,
    "rainbow-csv": true,
    "mcp-server-context7": true,
    "sql": true,
  },
}
